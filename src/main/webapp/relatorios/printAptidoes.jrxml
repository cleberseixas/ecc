	<queryString><![CDATA[select * from (
select a.nome, d.area, e.cidade, 'Cancelamento com diligência' as natureza, count(*)
from analista a, movimentacaoParecer c, areaconhecimento d, comarca e
where a.idAnalista = c.analistaenvolvidod
and a.areaconhecimento = d.idAreaConhecimento and a.comarca = e.idComarca
and c.status = 'Cancelado' and certidaoDiligencia = true
and c.dataFinal >=  $P{pDataInicial} and c.dataFinal <= $P{pDataFinal}
group by a.nome, d.area, e.cidade
union
select a.nome, d.area, e.cidade, b.natureza, count(*)
from analista a, naturezaMovimentacao b, movimentacaoParecer c, areaconhecimento d, comarca e
where a.idAnalista = c.analistaenvolvido and
b.idNaturezaMovimentacao = c. naturezaMovimentacao
and a.areaconhecimento = d.idAreaConhecimento and a.comarca = e.idComarca
and b.natureza <> 'Observação' and c.status <> 'Cancelado' and
c.dataFinal >=  $P{pDataInicial} and c.dataFinal <= $P{pDataFinal}
group by a.nome, d.area, e.cidade, b.natureza
union
select a.nome, d.area, e.cidade, b.atividade, count(*)
from analista a, tipoAtividade b, atividadeavulsa c, areaconhecimento d, comarca e
where a.idAnalista = c.analista and
b.idTipoAtividade = c. tipoAtividade
and a.areaconhecimento = d.idAreaConhecimento and a.comarca = e.idComarca
and b.atividade <> 'Observação' and c.statusAtividade <> 'Cancelada'
and c.dataConclusao >= $P{pDataInicial} and c.dataConclusao <= $P{pDataFinal}
group by a.nome, d.area, e.cidade, b.atividade
) as tebela
order by nome]]></queryString>

